@prefix eg: <http://example.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

eg:Square
	a owl:Class ;
	rdfs:label "Square" ;
	owl:disjointWith eg:Circle .

eg:Circle
	a owl:Class ;
	rdfs:label "Circle" ;
	owl:disjointWith eg:Square .

eg:magicShape
	a eg:Square ,
		eg:Circle .

